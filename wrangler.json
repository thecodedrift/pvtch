{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "pvtch-api",
  "main": "src/index.ts",
  "compatibility_flags": ["nodejs_compat"],
  "compatibility_date": "2025-04-01",
  "upload_source_maps": true,
  "migrations": [
    {
      "new_sqlite_classes": ["MyDurableObject"],
      "tag": "v1"
    },
    {
      "tag": "v2",
      "renamed_classes": [
        {
          "from": "MyDurableObject",
          "to": "PvtchBackend"
        }
      ]
    }
  ],
  "durable_objects": {
    "bindings": [
      {
        "class_name": "PvtchBackend",
        "name": "PVTCH_BACKEND"
      }
    ]
  },
  "kv_namespaces": [
    {
      "binding": "PVTCH_KV",
      "id": "3588c48277284bfb8e673399f904b794"
    },
    {
      "binding": "PVTCH_ACCOUNTS",
      "id": "bc19b133947e4936b7ce4d808b27023e"
    }
  ],
  "ai": {
    "binding": "AI"
  },
  "observability": {
    "enabled": true
  }
}
